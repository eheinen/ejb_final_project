package br.com.fiap.ws;

import javax.jws.WebMethod;
import javax.jws.WebParam;
import javax.jws.WebService;

import br.com.fiap.entity.Comentarios;

@WebService
public interface ComentarioWS {

	@WebMethod
	void postarComentario(@WebParam(name="comentarios")Comentarios comentarios);
	@WebMethod
	Comentarios buscarComentarios();
}
